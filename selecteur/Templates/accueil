
local function background()
        term.setCursorPos(1,1)
        term.setBackgroundColor(8)
        term.clearLine()
        term.setCursorPos(1,2)
        term.setBackgroundColor(16384)
        term.clearLine()
        term.setCursorPos(1,3)
        term.setBackgroundColor(256)
        term.clearLine()
        term.setCursorPos(1,4)
        term.setBackgroundColor(8)
        term.clearLine()
