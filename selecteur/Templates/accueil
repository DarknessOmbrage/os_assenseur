end

local function background2()
        term.setCursorPos(1,9)
        term.setBackgroundColor(128)
        term.clearLine()
        term.setCursorPos(1,10)
        term.setBackgroundColor(128)
        term.clearLine()
        term.setCursorPos(1,11)
        term.setBackgroundColor(128)
        term.clearLine()


end

background()
background2()